{
  "name": "create-cdk-app",
  "version": "0.0.1",
  "keywords": [
    "cdk",
    "aws"
  ],
  "description": "Create CDK-powered apps with one command",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdk-tools/create-cdk-app"
  },
  "author": "Joe Snell <joepsnell@gmail.com>",
  "license": "MIT",
  "bin": {
    "create-cdk-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ncc build ./index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublish": "yarn release"
  },
  "devDependencies": {
    "ncc": "latest",
    "rimraf": "latest",
    "typescript": "latest"
  }
}
